<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.luoyuequan.mqerp.mapper.InventoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.luoyuequan.mqerp.entity.Inventory">
        <id column="product_id" property="productId" />
        <result column="warehouse_id" property="warehouseId" />
        <result column="stock_quantity" property="stockQuantity" />
        <result column="frozen_quantity" property="frozenQuantity" />
        <result column="is_out_stock_notice" property="outStockNotice" />
        <result column="version" property="version" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        product_id, warehouse_id, stock_quantity, frozen_quantity, is_out_stock_notice, version
    </sql>

</mapper>
