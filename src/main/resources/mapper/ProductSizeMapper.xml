<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.luoyuequan.mqerp.mapper.ProductSizeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.luoyuequan.mqerp.entity.ProductSize">
        <id column="size_id" property="sizeId" />
        <result column="size_value" property="sizeValue" />
        <result column="queue_number" property="queueNumber" />
        <result column="unit_value" property="unitValue" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="is_deleted" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        size_id, size_value, queue_number, unit_value, create_time, update_time, is_deleted
    </sql>

</mapper>
