<view class="zai-box"><image class="zai-logo" src="../../static/zaizai-login/logo.png" mode="aspectFit"></image><view class="zai-title"></view><view class="zai-form"><input class="zai-input" type="number" placeholder-class="{{true}}" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/><input class="zai-input" password="{{true}}" placeholder-class="{{true}}" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="zai-btn" bindtap="__e">立即登录</button></view></view>